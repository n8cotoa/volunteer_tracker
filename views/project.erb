<h3><%=@project.title%></h3>
<div class="row">
  <div class="col-md-6">
    <h4>Here are the current volunteers</h3>
    <% if @volunteers.any? %>
      <ul>
        <% @volunteers.each do |volunteer| %>
          <li><a href="/volunteer/<%= volunteer.id %>"><%=volunteer.name%></a></li>
        <% end %>
      </ul>
    <% else %>
        <p>There are currently no volunteers</p>
    <% end %>
  </div>
  <div class="col-md-6">

  </div>
</div>
